  "workspaces": [
    "apps/*",
    "shared"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:all": "npm-run-all --parallel start:storefront start:catalog start:orders start:analytics start:shell:after-remotes",
    "wait:remotes": "wait-on http://localhost:3001/remoteEntry.js http://localhost:3002/remoteEntry.js http://localhost:3003/remoteEntry.js http://localhost:3004/remoteEntry.js",
    "start:shell:after-remotes": "npm run wait:remotes && npm run start:shell",
    "start:shell": "npm start --workspace=apps/shell",
    "start:storefront": "npm start --workspace=apps/storefront",
    "start:catalog": "npm start --workspace=apps/catalog",
    "start:orders": "npm start --workspace=apps/orders",
    "start:analytics": "npm start --workspace=apps/analytics"
  },
